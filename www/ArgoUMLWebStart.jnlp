<?xml version="1.0" encoding="utf-8"?>
      <!-- JNLP File for launching ArgoUML with WebStart -->
      <jnlp
        spec="1.0+"
        codebase="http://argouml.tigris.org/files/4/0/095/"
        href="http://argouml.tigris.org/files/4/0/095/ArgoUMLWebStart.jnlp">
        <information>
          <title>ArgoUML</title>
          <vendor>Tigris.org (Open Source)</vendor>
          <homepage href="http://argouml.tigris.org/"/>
          <description>ArgoUML Tigris Application</description>
          <description kind="short">The ArgoUML Swing based editor</description>
          <icon href="images/notepad.gif"/>
          <offline-allowed/>
        </information>
        <security>
            <all-permissions/>
        </security>
        <resources>
          <j2se version="1.4 1.4.0-beta3 1.4.0-beta2 1.3 1.2"/>
          <jar href="http://argouml.tigris.org/files/4/0/095/argouml.jar" main="true" download="eager"/>
          <jar href="http://argouml.tigris.org/files/4/0/095/xerces.jar"/>
          <jar href="http://argouml.tigris.org/files/4/0/095/antlrall.jar"/>
          <jar href="http://argouml.tigris.org/files/4/0/095/gef.jar"/>
          <jar href="http://argouml.tigris.org/files/4/0/095/ocl-argo.jar"/>
          <jar href="http://argouml.tigris.org/files/4/0/095/nsuml.jar"/>
          <jar href="http://argouml.tigris.org/files/4/0/095/log4j.jar"/>
        </resources>
        <application-desc main-class="org.argouml.application.Main"/>
      </jnlp>
