<?xml version="1.0"?>
<!DOCTYPE TemplateSet [
  <!ELEMENT TemplateSet (macro | template)* >
  <!ELEMENT macro (#PCDATA)>
  <!ATTLIST macro name ID #REQUIRED>
  <!ELEMENT template (#PCDATA)>
  <!ATTLIST template class CDATA #REQUIRED
                     guard CDATA #IMPLIED>
]>
<!-- This is XMI.tee, for Argo/UML models to generate XMI -->
<!-- files.  "tee" = Template with Embedded Expressions. -->

<!-- Original Author: jrobbins@ics.uci.edu -->
<!-- $Id$ -->


<!-- TO DO LIST: -->
<!-- 1. Behavior for model elments, context for state machines -->
<!-- 2. Proper replication of certain tags -->
<!-- 3. Complete all missing template  -->

<TemplateSet>

<!-- - - - - - - - Chapter 0: Argo/UML specific   - - - - - - - - - -->


<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
<template class="uci.uml.ui.Project">
<![CDATA[
<?xml version = "1.0" encoding = "ISO-8859-1" ?>
<!DOCTYPE XMI SYSTEM "uml.dtd" >
<XMI>
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>Argo/UML</XMI.exporter>
      <XMI.exporterVersion>0.7.0</XMI.exporterVersion> 
    </XMI.documentation>
    <XMI.metamodel name="uml" version="1.1"/>
  </XMI.header>
  <XMI.content>
    <ocl>self.models</ocl>
    <ocl>self.definedTypesVector</ocl>
  </XMI.content>
</XMI>
]]>
</template>
<!-- needs-more-work: generate version number -->
<!-- needs-more-work: shortDescription longDescription owner notice -->


<!-- - - - - - - - Chapter 7   - - - - - - - - - -->

<!-- ignored meta-classes and associations: provision, requirement,  -->
<!-- templates, template parameters -->

<!-- - - - - - - - Chapter 4   - - - - - - - - - -->

<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
