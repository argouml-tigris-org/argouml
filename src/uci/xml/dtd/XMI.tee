<!-- realizations is: <ocl>self.realization.subtype.name.body</ocl> -->
<specification> <XMI.reference target="<ocl>self.specification.supertype.id</ocl>"/> </specification>
<!-- specification is: <ocl>self.specification.supertype.name.body</ocl> -->
]]>
</macro>
