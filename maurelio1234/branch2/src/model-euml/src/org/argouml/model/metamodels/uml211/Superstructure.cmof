      <ownedAttribute xmi:type="cmof:Property" xmi:id="UseCases-Include-addition" name="addition" type="UseCases-UseCase" association="UseCases-A_addition_include" subsettedProperty="Classes-Kernel-DirectedRelationship-target">
        <ownedComment xmi:type="cmof:Comment" xmi:id="UseCases-Include-addition-_ownedComment.0" annotatedElement="UseCases-Include-addition">
          <body>References the use case that is to be included.&#xD;
</body>
        </ownedComment>
      </ownedAttribute>
