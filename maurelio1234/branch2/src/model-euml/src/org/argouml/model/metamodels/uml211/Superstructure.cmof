        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Element-ownedComment" name="ownedComment" lower="0" upper="*" type="Classes-Kernel-Comment" association="Classes-Kernel-A_ownedComment_owningElement" subsettedProperty="Classes-Kernel-Element-ownedElement" isComposite="true">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Classes-Kernel-Element-ownedComment-_ownedComment.0" annotatedElement="Classes-Kernel-Element-ownedComment">
