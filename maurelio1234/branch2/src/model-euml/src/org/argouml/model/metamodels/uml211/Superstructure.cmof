        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Namespace-elementImport" name="elementImport" lower="0" upper="*" type="Classes-Kernel-ElementImport" association="Classes-Kernel-A_elementImport_importingNamespace" subsettedProperty="Classes-Kernel-Element-ownedElement" isComposite="true">
