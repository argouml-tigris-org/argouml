        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Namespace-ownedMember" name="ownedMember" lower="0" upper="*" type="Classes-Kernel-NamedElement" isDerivedUnion="true" association="Classes-Kernel-A_ownedMember_namespace" subsettedProperty="Classes-Kernel-Namespace-member Classes-Kernel-Element-ownedElement" isReadOnly="true" isComposite="true" isDerived="true">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Classes-Kernel-Namespace-ownedMember-_ownedComment.0" annotatedElement="Classes-Kernel-Namespace-ownedMember">
            <body>A collection of NamedElements owned by the Namespace.</body>
          </ownedComment>
