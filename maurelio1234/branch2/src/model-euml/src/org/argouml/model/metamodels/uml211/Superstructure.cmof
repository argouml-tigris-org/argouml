          </ownedComment>
        </ownedAttribute>
        <ownedAttribute xmi:type="cmof:Property" xmi:id="Activities-ExtraStructuredActivities-ExpansionNode-regionAsInput" name="regionAsInput" lower="0" type="Activities-ExtraStructuredActivities-ExpansionRegion" association="Activities-ExtraStructuredActivities-A_inputElement_regionAsInput">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Activities-ExtraStructuredActivities-ExpansionNode-regionAsInput-_ownedComment.0" annotatedElement="Activities-ExtraStructuredActivities-ExpansionNode-regionAsInput">
