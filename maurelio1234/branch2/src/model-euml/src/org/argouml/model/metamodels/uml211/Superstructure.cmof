        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-BehavioralFeature-raisedException" name="raisedException" lower="0" upper="*" type="Classes-Kernel-Type" association="Classes-Kernel-A_raisedException_behavioralFeature">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Classes-Kernel-BehavioralFeature-raisedException-_ownedComment.0" annotatedElement="Classes-Kernel-BehavioralFeature-raisedException">
