          <body>References the use cases owned by this classifier.</body>
        </ownedComment>
      </ownedAttribute>
      <ownedAttribute xmi:type="cmof:Property" xmi:id="UseCases-Classifier-useCase" name="useCase" lower="0" upper="*" type="UseCases-UseCase" association="UseCases-A_subject_useCase">
        <ownedComment xmi:type="cmof:Comment" xmi:id="UseCases-Classifier-useCase-_ownedComment.0" annotatedElement="UseCases-Classifier-useCase">
