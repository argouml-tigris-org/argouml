        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Namespace-member" name="member" lower="0" upper="*" type="Classes-Kernel-NamedElement" isDerivedUnion="true" association="Classes-Kernel-A_member_namespace" isReadOnly="true" isDerived="true">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Classes-Kernel-Namespace-member-_ownedComment.0" annotatedElement="Classes-Kernel-Namespace-member">
            <body>A collection of NamedElements identifiable within the Namespace, either by being owned or by being introduced by importing or inheritance.</body>
