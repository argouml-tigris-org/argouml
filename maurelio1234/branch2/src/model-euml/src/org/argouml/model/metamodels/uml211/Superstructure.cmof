        </ownedAttribute>
        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Generalization-specific" name="specific" type="Classes-Kernel-Classifier" association="Classes-Kernel-A_generalization_specific" subsettedProperty="Classes-Kernel-DirectedRelationship-source Classes-Kernel-Element-owner">
