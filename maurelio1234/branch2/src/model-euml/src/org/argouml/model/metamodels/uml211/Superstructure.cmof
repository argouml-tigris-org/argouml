          <specification xmi:type="cmof:OpaqueExpression" xmi:id="Classes-Kernel-Element-has_owner-_specification">
            <language>OCL</language>
            <body>self.mustBeOwned() implies owner->notEmpty()</body>
          </specification>
        </ownedRule>
        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Element-ownedElement" name="ownedElement" lower="0" upper="*" type="Classes-Kernel-Element" isDerivedUnion="true" association="Classes-Kernel-A_ownedElement_owner" isReadOnly="true" isComposite="true" isDerived="true">
