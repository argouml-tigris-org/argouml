        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Slot-owningInstance" name="owningInstance" type="Classes-Kernel-InstanceSpecification" association="Classes-Kernel-A_slot_owningInstance" subsettedProperty="Classes-Kernel-Element-owner">
