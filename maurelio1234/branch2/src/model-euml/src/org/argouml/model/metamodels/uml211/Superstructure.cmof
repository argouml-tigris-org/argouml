          <ownedParameter xmi:type="cmof:Parameter" xmi:id="Classes-Kernel-Element-allOwnedElements-_member.1" lower="0" upper="*" type="Classes-Kernel-Element" direction="return"/>
        </ownedOperation>
        <ownedOperation xmi:type="cmof:Operation" xmi:id="Classes-Kernel-Element-mustBeOwned" name="mustBeOwned" isQuery="true" bodyCondition="Classes-Kernel-Element-mustBeOwned-spec">
