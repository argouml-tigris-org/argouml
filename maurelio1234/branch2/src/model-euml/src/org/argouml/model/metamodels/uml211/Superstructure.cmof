        <ownedRule xmi:type="cmof:Constraint" xmi:id="Classes-Kernel-Association-association_ends" name="association_ends" constrainedElement="Classes-Kernel-Association">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Classes-Kernel-Association-association_ends-_ownedComment.0" annotatedElement="Classes-Kernel-Association-association_ends">
