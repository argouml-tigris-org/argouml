        <ownedOperation xmi:type="cmof:Operation" xmi:id="Activities-StructuredActivities-Variable-isAccessibleBy" name="isAccessibleBy" isQuery="true" bodyCondition="Activities-StructuredActivities-Variable-isAccessibleBy-spec">
