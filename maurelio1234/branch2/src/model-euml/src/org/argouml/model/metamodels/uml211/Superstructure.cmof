          <specification xmi:type="cmof:OpaqueExpression" xmi:id="Actions-IntermediateActions-CreateObjectAction-classifier_not_abstract-_specification">
            <language>OCL</language>
            <body>not (self.classifier.isAbstract = #true)</body>
          </specification>
        </ownedRule>
