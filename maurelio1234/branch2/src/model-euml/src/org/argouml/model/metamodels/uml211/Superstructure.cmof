          </ownedComment>
        </ownedAttribute>
      </ownedMember>
      <ownedMember xmi:type="cmof:Class" xmi:id="Activities-ExtraStructuredActivities-ExecutableNode" name="ExecutableNode" isAbstract="true" superClass="Activities-StructuredActivities-ActivityNode">
