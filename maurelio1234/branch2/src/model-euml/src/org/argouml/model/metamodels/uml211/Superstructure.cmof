        <ownedRule xmi:type="cmof:Constraint" xmi:id="Classes-Kernel-Namespace-members_distinguishable" name="members_distinguishable" constrainedElement="Classes-Kernel-Namespace">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Classes-Kernel-Namespace-members_distinguishable-_ownedComment.0" annotatedElement="Classes-Kernel-Namespace-members_distinguishable">
            <body>All the members of a Namespace are distinguishable within it.</body>
