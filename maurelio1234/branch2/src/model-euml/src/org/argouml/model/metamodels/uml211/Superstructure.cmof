        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Namespace-packageImport" name="packageImport" lower="0" upper="*" type="Classes-Kernel-PackageImport" association="Classes-Kernel-A_packageImport_importingNamespace" subsettedProperty="Classes-Kernel-Element-ownedElement" isComposite="true">
