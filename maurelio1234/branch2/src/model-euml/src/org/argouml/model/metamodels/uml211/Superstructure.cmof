      <ownedMember xmi:type="cmof:Class" xmi:id="StateMachines-BehaviorStateMachines-TimeEvent" name="TimeEvent">
        <ownedComment xmi:type="cmof:Comment" xmi:id="StateMachines-BehaviorStateMachines-TimeEvent-_ownedComment.0" annotatedElement="StateMachines-BehaviorStateMachines-TimeEvent">
          <body>A time event can be defined relative to entering the current state of the executing state machine.</body>
        </ownedComment>
        <ownedRule xmi:type="cmof:Constraint" xmi:id="StateMachines-BehaviorStateMachines-TimeEvent-starting_time" name="starting_time" constrainedElement="StateMachines-BehaviorStateMachines-TimeEvent">
