          </ownedComment>
          <specification xmi:type="cmof:OpaqueExpression" xmi:id="Actions-IntermediateActions-CreateObjectAction-same_type-_specification">
            <language>OCL</language>
            <body>self.result.type = self.classifier</body>
          </specification>
