        <ownedRule xmi:type="cmof:Constraint" xmi:id="Interactions-Fragments-InteractionOperand-guard_contain_references" name="guard_contain_references" constrainedElement="Interactions-Fragments-InteractionOperand">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Interactions-Fragments-InteractionOperand-guard_contain_references-_ownedComment.0" annotatedElement="Interactions-Fragments-InteractionOperand-guard_contain_references">
