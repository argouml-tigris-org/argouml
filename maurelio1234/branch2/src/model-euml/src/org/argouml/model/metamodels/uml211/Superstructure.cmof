        <ownedAttribute xmi:type="cmof:Property" xmi:id="AuxiliaryConstructs-InformationFlows-InformationFlow-informationSource" name="informationSource" upper="*" type="Classes-Kernel-NamedElement" association="AuxiliaryConstructs-InformationFlows-A_informationSource_informationFlow" subsettedProperty="Classes-Kernel-DirectedRelationship-source">
          <ownedComment xmi:type="cmof:Comment" xmi:id="AuxiliaryConstructs-InformationFlows-InformationFlow-informationSource-_ownedComment.0" annotatedElement="AuxiliaryConstructs-InformationFlows-InformationFlow-informationSource">
            <body>Defines from which source the conveyed InformationItems are initiated.</body>
          </ownedComment>
        </ownedAttribute>
        <ownedAttribute xmi:type="cmof:Property" xmi:id="AuxiliaryConstructs-InformationFlows-InformationFlow-informationTarget" name="informationTarget" upper="*" type="Classes-Kernel-NamedElement" association="AuxiliaryConstructs-InformationFlows-A_informationTarget_informationFlow" subsettedProperty="Classes-Kernel-DirectedRelationship-target">
          <ownedComment xmi:type="cmof:Comment" xmi:id="AuxiliaryConstructs-InformationFlows-InformationFlow-informationTarget-_ownedComment.0" annotatedElement="AuxiliaryConstructs-InformationFlows-InformationFlow-informationTarget">
            <body>Defines to which target the conveyed InformationItems are directed.</body>
          </ownedComment>
        </ownedAttribute>
        <ownedAttribute xmi:type="cmof:Property" xmi:id="AuxiliaryConstructs-InformationFlows-InformationFlow-realizingActivityEdge" name="realizingActivityEdge" lower="0" upper="*" type="Activities-BasicActivities-ActivityEdge" association="AuxiliaryConstructs-InformationFlows-A_realizingActivityEdge_informationFlow">
          <ownedComment xmi:type="cmof:Comment" xmi:id="AuxiliaryConstructs-InformationFlows-InformationFlow-realizingActivityEdge-_ownedComment.0" annotatedElement="AuxiliaryConstructs-InformationFlows-InformationFlow-realizingActivityEdge">
            <body>Determines which ActivityEdges will realize the specified flow.</body>
          </ownedComment>
        </ownedAttribute>
        <ownedAttribute xmi:type="cmof:Property" xmi:id="AuxiliaryConstructs-InformationFlows-InformationFlow-realizingConnector" name="realizingConnector" lower="0" upper="*" type="CompositeStructures-InternalStructures-Connector" association="AuxiliaryConstructs-InformationFlows-A_realizingConnector_informationFlow">
