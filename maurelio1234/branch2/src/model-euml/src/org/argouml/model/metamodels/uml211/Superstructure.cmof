          <ownedComment xmi:type="cmof:Comment" xmi:id="Classes-Kernel-Element-not_own_self-_ownedComment.0" annotatedElement="Classes-Kernel-Element-not_own_self">
            <body>An element may not directly or indirectly own itself.</body>
          </ownedComment>
          <specification xmi:type="cmof:OpaqueExpression" xmi:id="Classes-Kernel-Element-not_own_self-_specification">
