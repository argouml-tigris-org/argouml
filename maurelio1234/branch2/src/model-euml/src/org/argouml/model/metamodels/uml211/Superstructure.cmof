        </ownedComment>
        <ownedAttribute xmi:type="cmof:Property" xmi:id="Classes-Kernel-Generalization-isSubstitutable" name="isSubstitutable" lower="0">
          <ownedComment xmi:type="cmof:Comment" xmi:id="Classes-Kernel-Generalization-isSubstitutable-_ownedComment.0" annotatedElement="Classes-Kernel-Generalization-isSubstitutable">
